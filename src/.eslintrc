{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": ["airbnb/base"],
    "rules": {
        "semi" : "off",
        "no-plusplus" : "off",
        "eol-last" : "off",
        "max-len" : ["error", 150],
        "no-tabs" : "off",
        "indent" : [2, "tab"],
        "prefer-destructuring" : "off"
    }
}